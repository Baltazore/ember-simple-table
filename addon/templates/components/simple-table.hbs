<thead>
    {{simple-table-header rowData=headerRow keys=columns sortBy=(action 'sortBy')}}
</thead>

<tbody>
    {{#each bodyRows as |rowData| }}
        {{#if hasBlock}}
            {{yield rowData}}
        {{else}}
            {{simple-table-row rowData=rowData keys=columns}}
        {{/if}}
    {{/each}}
</tbody>
